<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.NaolLegesse.accounttcreation">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_without_text"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccounttCreation">
        <activity
            android:name="com.NaolLegesse.accounttcreation.SplashActivity"
            android:exported="true"
            android:theme="@style/SplashActivity"
            android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <activity
            android:name="com.NaolLegesse.accounttcreation.IndividualCommentDisplay"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.IndividualProfessorCourseRating"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.ProfessorScreen"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.DisplayIndividualUserInput"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.SearchViewProfByNameActivity"
            android:exported="true"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.ProfileActivity"
            android:configChanges="orientation|screenSize"
            android:exported="true"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.DisplayUserInput"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.UserInput"
            android:exported="true"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.ResetPassword"
            android:exported="true"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.RecycleViewDB"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.RegisterActivity"
            android:exported="true"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.NaolLegesse.accounttcreation.LogInActivity"
            android:exported="true"
            android:theme="@style/Theme.AccounttCreation.NoActionBar"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.NaolLegesse.accounttcreation.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>